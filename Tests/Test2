INICIO:

DCL xcv;
DCL vf;
DCL bv DEFAULT 10;
DCL mnb DEFAULT 10;

EnCaso
	Cuando bv = 1 EnTons
		{
            INC(bv, 1);
		}
	Cuando bv = 2 EnTons
		{
            INC(bv, 2);
		}
	Cuando bv < mnb EnTons
		{
		INC(bv,4);

		}

	SiNo
	{
        INC(bv, 4);
	}
Fin-EnCaso;


FIN;

PROC  hola()
inicio:

fin;


